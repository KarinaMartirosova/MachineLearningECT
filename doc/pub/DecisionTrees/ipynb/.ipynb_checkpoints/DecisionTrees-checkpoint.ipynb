    "\tdecisionTree = DecisionTree(sample, attributes, labels)\n",
    "\tprint(\"System entropy {}\".format(decisionTree.entropy))\n",
    "\tdecisionTree.id3()\n",
    "\tdecisionTree.printTree()\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "\ttest()"
